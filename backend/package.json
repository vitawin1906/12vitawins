{
  "name": "vitawin-backend",
  "version": "1.0.0",
  "description": "VitaWin Backend (Node.js, Express, Drizzle ORM, PostgreSQL)",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "dotenv -e .env -- tsx -r tsconfig-paths/register src/index.ts",
    "dev:watch": "dotenv -e .env -- tsx -r tsconfig-paths/register watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "start:env": "dotenv -e .env -- node --experimental-specifier-resolution=node dist/index.js",
    "psql": "sh -c 'psql \"$(grep ^DATABASE_URL .env | cut -d '=' -f2-)\"'",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist",
    "docs:build": "tsx src/docs/openapi.ts",
    "docs:build:dev": "OPENAPI_INCLUDE_LOCALHOST=true tsx src/docs/openapi.ts",
    "docs:build:prod": "tsx src/docs/openapi.ts",
    "docs:validate": "redocly lint openapi.json",
    "docs:coverage": "tsx scripts/check-openapi-coverage.ts",
    "docs:preview": "node -e \"require('http').createServer((_,res)=>res.end('Open http://localhost:3000/docs')).listen(3001)\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "dotenv -e .env -- tsx src/db/seed.ts",
    "db:reset": "rimraf drizzle/migrations && npm run db:generate && npm run db:migrate && npm run db:seed:new"
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "camelcase-keys": "^10.0.0",
    "cloudinary": "^1.41.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "express": "^4",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^166.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pino-http": "^9.0.0",
    "redis": "^4.7.0",
    "redlock": "^5.0.0-beta.2",
    "streamifier": "^0.1.1",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@eslint/js": "^9.18.0",
    "@redocly/cli": "^2.10.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.15.5",
    "@types/redlock": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "dependency-cruiser": "^17.0.2",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "fast-glob": "^3.3.3",
    "madge": "^8.0.0",
    "pg": "^8.16.3",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "ts-unused-exports": "^11.0.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
